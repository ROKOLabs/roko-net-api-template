{
  "$schema": "http://json.schemastore.org/template",
  "author": "Rokolabs",
  "classifications": [ "Web" ],
  "identity": "Roko.Api",
  "name": "Roko.Api",
  "shortName": "roko-api",
  "preferNameDirectory": true,
  "sourceName": "Roko.Template",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "sources":[
    {
      "modifiers":[
        {
          "exclude":[
            "**/.git/**",
            "**/.vs/**",
            "**/.idea/**"
          ]
        }
      ]
    }
  ],
  "symbols": {
    "Database": {
      "type": "parameter",
      "description": "The target database for the project.",
      "datatype": "choice",
      "allowMultipleValues": false,
      "enableQuotelessLiterals": true,
      "choices": [
        {
          "choice": "MsSql",
          "description": "Microsoft SQL Server relational database."
        },
        {
          "choice": "Postgres",
          "description": "The Postgres SQL object-relational database."
        }
      ],
      "defaultValue": "Postgres",
      "fileRename": "MyDb",
      "replaces": "MyDb"
    },
    "Postgres": {
      "type": "computed",
      "value": "(Database==\"Postgres\")"
    },
    "MsSql": {
      "type": "computed",
      "value": "(Database==\"MsSql\")"
    },
    "IdUniqueFolderMsSql": {
      "type": "generated",
      "generator": "guid",
      "replaces": "UniqueFolderMsSql"
    }
  }
}